    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="startKapital" type="Real" array="False" size=""/>
            <input variable="startKapital"/>
            <declare name="zielkapital" type="Real" array="False" size=""/>
            <input variable="zielkapital"/>
            <declare name="prozentsatz" type="Real" array="False" size=""/>
            <input variable="prozentsatz"/>
            <declare name="aktuelleskapital" type="Real" array="False" size=""/>
            <assign variable="aktuelleskapital" expression="startKapital"/>
            <declare name="jahr" type="Real" array="False" size=""/>
            <assign variable="jahr" expression="0"/>
            <while expression="aktuelleskapital &lt; zielkapital">
                <assign variable="aktuelleskapital" expression="aktuelleskapital * (prozentsatz+100)/100"/>
                <assign variable="jahr" expression="jahr + 1"/>
            </while>
            <output expression="&quot;Nach &quot; &amp; jahr &amp; &quot; Jahren betr&#228;gt Ihr Kapital &quot; &amp; aktuelleskapital &amp; &quot; Euro&quot;" newline="True"/>
        </body>
    </function>
</flowgorithm>
